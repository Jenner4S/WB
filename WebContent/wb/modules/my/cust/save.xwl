{"hidden":true,"children":[{"configs":{"serverScript":"var id, ids = [],\nrecs = Wb.decode(request.getParameter('create'));\n\napp.run('select 1 from T_BUS_CUSTOMER where CODE={?CODE?}', {\n  errorText: '用户代码 \"' + recs.CODE + '\" 已经存在。'\n});\nWb.each(recs, function(rec) {\n  id = SysUtil.getId();\n  rec.CREATER = id;\n  rec.UPDATER = id;\n  rec.CREATER_DATE = new JavaDate();\n  rec.UPDATE_DATE = new JavaDate();\n  rec.DEL = 'N';\n});\nrequest.setAttribute('create', Wb.encode(recs)); //重置create记录\napp.update({\n  tableName: 'T_BUS_CUSTOMER'\n});\n//app.send(ids); //把id列表发送到客户端以同步到表格\n//app.send({create:createRows,update:updatedRows,otherParams:params});//如果需要同时同步新建和修改的记录以及其他参数","itemId":"module"},"expanded":true,"children":[],"type":"module"}],"roles":{"manager":1,"user":1},"title":"保存","iconCls":"","inframe":false,"pageLink":""}